{
  "/api": {
    "target": "http://localhost:3000",
    "secure": false,
    "changeOrigin": true
  },
  "/elasticsearch": {
    "target": "https://YOUR_ELASTICSEARCH_ENDPOINT.es.us-central1.gcp.elastic.cloud:443",
    "secure": true,
    "changeOrigin": true,
    "logLevel": "debug",
    "headers": {
      "Authorization": "ApiKey YOUR_ELASTICSEARCH_API_KEY_HERE"
    },
    "pathRewrite": {
      "^/elasticsearch": ""
    }
  }
}

